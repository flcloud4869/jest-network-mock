{
  "name": "jest-network-mock",
  "version": "2.0.0",
  "description": "NetworkInformation API mock for Jest",
  "repository": "github:suiyun39/jest-network-mock",
  "license": "MIT",
  "keywords": [
    "jest",
    "jest-mock",
    "jsdom"
  ],
  "type": "module",
  "packageManager": "pnpm@8.12.0",
  "scripts": {
